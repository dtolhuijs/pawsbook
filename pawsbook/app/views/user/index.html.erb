<div>
<h1>Users</h1>

<% @users.each do |user| %>
    <p><%= link_to user.name, user_path( user.id ) %></p>

    <!-- <img src = "<%= user.picture%>"> -->
    <% user.pictures.each do |pic| %>
      <%= pic.id %>
      <%= image_tag pic.name.url %>
    <% end %>
    <h2><%= user.name %></h2>
    <p> <%= user.gender%></p>
<% end %>

<%= link_to "[New User]", new_user_path %>
</div>
